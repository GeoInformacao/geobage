remove.packages("geobage", lib="~/R/win-library/3.6")
devtools::install_github("Prof-Rodrigo-Silva/geobage")
#install.packages("Rcpp")
library(geobage)
bg = read_info_bg()
bg = read_intermediate_region_bg()
bg = read_micro_region_bg()
bg = read_conservation_unit_bg()
bg = read_biome_pampa()
c <- read_urban_area_bg()
plot(bg)
library(leaflet)
map = leaflet(c)%>%addTiles()
map %>% addPolygons()
